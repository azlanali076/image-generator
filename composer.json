{
    "name": "azlanali076/image-generator",
    "description": "A Laravel package for generating images using the OpenAI API. This package provides an easy-to-use interface for interacting with the OpenAI API to generate images based on text prompts.",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Azlanali076\\ImageGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "azlanali076",
            "email": "azlanali076@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^10.48",
        "guzzlehttp/guzzle": "^7.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azlanali076\\ImageGenerator\\Providers\\ImageGeneratorServiceProvider"
            ],
            "aliases": {
                "ImageGenerator": "Azlanali076\\ImageGenerator\\Facades\\ImageGenerator"
            }
        }
    }
}
